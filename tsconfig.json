{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "isolatedModules": true,
        "jsx": "react-jsx",
        "module": "Preserve",
        "target": "ES2022",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "#/*": ["src/*"]
        }
    }
}